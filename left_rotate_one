import java.util.Arrays;

public class left_rotate {
    public static void main(String[] args) {
        int[] arr = {9,6,7,44,-5,0,8,0,667,5};
        System.out.println(Arrays.toString(arr));
        int temp = arr[0];

        for(int i=1;i<arr.length;i++){
            arr[i-1] = arr[i];
        }
        arr[arr.length-1] = temp;
        System.out.println(Arrays.toString(arr));
    }
}
