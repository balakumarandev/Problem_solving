import java.util.Arrays;

public class move_zeros {
    public static void main(String[] args) {
        int[] arr = {0,4,5,6,0,12,0};
        int s = 0;
        for(int i: arr){
            if(i!=0){
                arr[s] = i;
                s++;
            }
        }
        for(int k=s;k<arr.length;k++){
            arr[k] = 0;
        }
        System.out.println(Arrays.toString(arr));
    }
}
