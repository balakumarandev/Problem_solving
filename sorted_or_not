public class sorted_or_not {
    public static void main(String[] args) {
        int[] arr={-5,6,-7,8,9,90};
        for(int i=0;i<arr.length-1;i++){
            if(arr[i] > arr[i+1]){
                System.out.println("false");
                return;
            }
        }
        System.out.println("true");
    }
}
